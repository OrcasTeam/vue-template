{
  "name": "vue-webpack",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/OrcasTeam/vue-template.git",
  "author": "yanzhangshuai <yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "build:br": "npm-run-all lint && cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" WEBPACK_BUILD_COMPRESS=brotli  webpack",
    "build:gzip": "npm-run-all lint && cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" WEBPACK_BUILD_COMPRESS=gzip  webpack",
    "report": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\"  WEBPACK_REPORT=true webpack",
    "lint:js": "eslint --config .eslintrc.js  ./{src,test}/**/*.{ts,tsx,vue} --fix",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx} --fix",
    "lint": "npm-run-all --parallel lint:*",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.26",
    "@vueuse/core": "^7.4.0",
    "axios": "^0.24.0",
    "bucket-storage": "^0.4.6",
    "dayjs": "^1.10.7",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.8",
    "qs": "^6.10.2",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.5.4",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/plugin-proposal-function-bind": "^7.16.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/lodash-es": "^4.17.5",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^17.0.2",
    "@types/qs": "^6.9.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "4.5.0",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.26",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "compression-webpack-plugin": "^9.2.0",
    "core-js": "^3.20.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.3",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "portfinder": "^1.0.28",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "regenerator-runtime": "^0.13.9",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-node": "^10.4.0",
    "vue-loader": "^17.0.0",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": {
    "development": [
      "chrome >= 49"
    ],
    "production": [
      "chrome >= 49"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
