    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.1.4",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
