{
  "name": "spa-vite-template",
  "version": "1.0.0",
  "repository": "https://github.com/yanzhangshuai/spa-vue-template.git",
  "author": "yanzhangshuai <yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:skip-check": "vite build",
    "build": "vue-tsc --noEmit && vite build",
    "build:br": "vue-tsc --noEmit && cross-env VITE_BUILD_COMPRESS=brotli  vite build",
    "build:gzip": "vue-tsc --noEmit && cross-env VITE_BUILD_COMPRESS=gzip  vite build",
    "serve": "npm-run-all build:skip-check && vite preview",
    "lint:js": "eslint --config .eslintrc.js  ./{src,test,type,build}/**/*.{ts,tsx,vue} --fix",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{css,less,vue} --fix",
    "lint": "npm-run-all --parallel lint:*",
    "test": "jest --passWith-no-tests",
    "prepare": "husky install",
    "report": "cross-env VITE_REPORT=true npm-run-all build:skip-check",
    "report:windi": "windicss-analysis",
    "report:test": "jest --passWith-no-tests --coverage  --coverageDirectory ./report/jest/coverage --reporters jest-stare"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.31",
    "@vueuse/core": "^8.1.2",
    "axios": "^0.26.1",
    "bucket-storage": "^0.4.6",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.0",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.12",
    "query-string": "^7.1.1",
    "ramda": "^0.28.0",
    "typescript": "^4.6.3",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "windicss": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-dynamic-import-vars": "^1.4.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.4.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@types/ramda": "^0.28.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.5",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-stare": "^2.3.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.1",
    "rollup": "^2.70.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.6.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "unplugin-vue-components": "^0.18.4",
    "vite": "^2.8.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-windicss": "^1.8.3",
    "vue-tsc": "^0.33.9",
    "windicss-analysis": "^0.3.5"
  },
  "jest-stare": {
    "merge": "true",
    "resultDir": "report/jest",
    "log": "true",
    "resultHtml": "index.html",
    "resultJson": "index.json",
    "jestStareConfigJson": "jest-stare-config.json",
    "coverageLink": "report/jest/coverage/index.html"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
