{
  "name": "spa-vue-template",
  "version": "1.0.0",
  "repository": "https://github.com/yanzhangshuai/spa-vue-template.git",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "vue-tsc": "     vue-tsc --noEmit --skipLibCheck",
    "vite-build": "  vite build --config ./build/vite.config.ts",
    "dev": "         vite --config ./build/vite.config.ts",
    "build": "       npm-run-all vue-tsc && npm-run-all vite-build",
    "build:br": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=brotli npm-run-all vite-build",
    "build:gz": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=gzip   npm-run-all vite-build",
    "serve": "       npm-run-all vite-build && vite preview --config ./build/vite.preview.ts",
    "deploy": "      npm-run-all build:gz && docker build -t spa-vue-template -f ./deploy/Dockerfile .",
    "lint:js": "     eslint    ./{src,test,type,build}/**/*.{js,ts,tsx,vue} --fix --ignore-path ./standard/.eslintignore",
    "lint:style": "  stylelint ./src/**/*.{css,less,vue}           --fix",
    "lint": "        npm-run-all --parallel lint:*",
    "test": "        vitest --passWithNoTests --config ./test/vitest.config.ts",
    "report": "      cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
    "report:test": " vitest run --passWithNoTests --config ./test/vitest.config.ts --coverage  && open-cli ./report/test/index.html",
    "prepare": "     husky install"
  },
  "dependencies": {
    "@mwjz/asker": "^0.0.5",
    "@mwjz/pinia-storage": "^0.0.3",
    "@mwjz/utils": "^0.1.8",
    "@pinia/plugin-debounce": "^0.1.0",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.33",
    "query-string": "^8.1.0",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.37.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@pinia/testing": "^0.0.15",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^8.1.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.3",
    "@typescript-eslint/parser": "^5.55.0",
    "@unocss/preset-uno": "^0.50.6",
    "@unocss/vite": "^0.50.6",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.29.3",
    "@vue/runtime-core": "^3.2.47",
    "@vue/test-utils": "2.3.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.15",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "glob": "^9.3.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.1.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.4",
    "rollup": "^3.19.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^15.3.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^31.0.0",
    "terser": "^5.16.6",
    "ts-node": "^10.9.1",
    "unocss-preset-theme": "^0.5.0",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-static-copy": "^0.13.1",
    "vitest": "^0.29.3",
    "vue-tsc": "^1.2.0"
  },
  "prettier": "./standard/.prettierrc.js",
  "eslintConfig": {
    "extends": "./standard/.eslintrc.js"
  },
  "stylelint": {
    "extends": "./standard/.stylelintrc.js"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
