{
  "name": "spa-vue-template",
  "version": "1.0.0",
  "repository": "https://github.com/yanzhangshuai/spa-vue-template.git",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "vue-tsc": "     vue-tsc --noEmit --skipLibCheck",
    "vite-build": "  vite build --config ./build/vite.config.ts",
    "dev": "         vite --config ./build/vite.config.ts",
    "build": "       npm-run-all vue-tsc && npm-run-all vite-build",
    "build:br": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=brotli npm-run-all vite-build",
    "build:gz": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=gzip   npm-run-all vite-build",
    "serve": "       npm-run-all vite-build && vite preview --config ./build/vite.preview.ts",
    "deploy": "      npm-run-all build:gz && docker build -t spa-vue-template -f ./deploy/Dockerfile .",
    "lint:js": "     eslint    ./{src,test,type,build}/**/*.{js,ts,tsx,vue} --fix --ignore-path ./standard/.eslintignore",
    "lint:style": "  stylelint ./src/**/*.{css,less,vue}           --fix",
    "lint": "        npm-run-all --parallel lint:*",
    "test": "        vitest --passWithNoTests --config ./test/vitest.config.ts",
    "report": "      cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
    "report:test": " vitest run --passWithNoTests --config ./test/vitest.config.ts --coverage  && open-cli ./report/test/index.html",
    "prepare": "     husky install"
  },
  "dependencies": {
    "@mwjz/asker": "^0.0.3",
    "@mwjz/pinia-storage": "^0.0.3",
    "@mwjz/utils": "^0.1.8",
    "@pinia/plugin-debounce": "^0.1.0",
    "@unocss/preset-uno": "^0.48.0",
    "@vueuse/core": "^9.9.0",
    "axios": "^1.2.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.28",
    "query-string": "^8.1.0",
    "typescript": "^4.9.4",
    "unocss": "^0.48.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.34.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^8.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.47.1",
    "@unocss/vite": "^0.48.0",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.26.2",
    "@vue/runtime-core": "^3.2.45",
    "@vue/test-utils": "2.2.6",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "glob": "^8.0.3",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.1.0",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.1",
    "rollup": "^3.8.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "stylelint": "^14.16.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "terser": "^5.16.1",
    "ts-node": "^10.9.1",
    "unocss-preset-theme": "^0.3.0",
    "vite": "^4.0.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pages": "^0.28.0",
    "vitest": "^0.26.2",
    "vue-tsc": "^1.0.18"
  },
  "prettier": "./standard/.prettierrc.js",
  "eslintConfig": {
    "extends": "./standard/.eslintrc.js"
  },
  "stylelint": {
    "extends": "./standard/.stylelintrc.js"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
