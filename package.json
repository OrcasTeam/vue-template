{
  "name": "spa-vue-template",
  "version": "1.0.0",
  "repository": "https://github.com/yanzhangshuai/spa-vue-template.git",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "vue-tsc": "     vue-tsc --noEmit --skipLibCheck",
    "vite-build": "  vite build --config ./build/vite.config.ts",
    "dev": "         vite --config ./build/vite.config.ts",
    "build": "       npm-run-all vue-tsc && npm-run-all vite-build",
    "build:br": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=brotli npm-run-all vite-build",
    "build:gz": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=gzip   npm-run-all vite-build",
    "serve": "       npm-run-all vite-build && vite preview --config ./build/vite.preview.ts",
    "deploy": "      npm-run-all build:gz && docker build -t spa-vue-template -f ./deploy/Dockerfile .",
    "lint:js": "     eslint    ./{src,test,type,build}/**/*.{ts,tsx,vue} --fix",
    "lint:style": "  stylelint ./src/**/*.{css,less,vue}           --fix",
    "lint": "        npm-run-all --parallel lint:*",
    "test": "        vitest --passWithNoTests --config ./test/vitest.config.ts",
    "report": "      cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
    "report:test": " vitest run --passWithNoTests --config ./test/vitest.config.ts --coverage  && open-cli ./report/test/index.html",
    "report:windi": "windicss-analysis",
    "prepare": "     husky install"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.5",
    "axios": "^0.26.1",
    "bucket-storage": "^0.4.6",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.0",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.13",
    "query-string": "^7.1.1",
    "typescript": "^4.6.3",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "windicss": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-dynamic-import-vars": "^1.4.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-legacy": "^1.8.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/runtime-core": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.19",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^14.6.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "ts-node": "^10.7.0",
    "vite": "^2.9.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-windicss": "^1.8.3",
    "vitest": "^0.9.0",
    "vue-tsc": "^0.33.9",
    "windicss-analysis": "^0.3.5"
  },
  "prettier": "./standard/.prettierrc.js",
  "eslintConfig": {
    "extends": "./standard/.eslintrc.js"
  },
  "stylelint": {
    "extends": "./standard/.stylelintrc.js"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
