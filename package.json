{
  "name": "pc-vite-template",
  "version": "1.0.0",
  "repository": "https://github.com/OrcasTeam/vue-template.git",
  "author": "yanzhangshuai <yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "yarn lint-fix && vue-tsc --noEmit && vite build",
    "build:br": "yarn lint-fix && vue-tsc --noEmit && cross-env VITE_BUILD_COMPRESS=brotli  vite build",
    "build:gzip": "yarn lint-fix && vue-tsc --noEmit && cross-env VITE_BUILD_COMPRESS=gzip  vite build",
    "report": "cross-env VITE_REPORT=true  vite build",
    "lint:js": "eslint --config .eslintrc.js  ./{src,test}/**/*.{ts,tsx,vue}",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx}",
    "lint": "npm-run-all --parallel lint:*",
    "lint-fix:js": "eslint --config .eslintrc.js  ./src/**/*.{ts,tsx,vue} --fix",
    "lint-fix:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx} --fix",
    "lint-fix": "npm-run-all --parallel lint-fix:*",
    "test": "jest --passWithNoTests",
    "test:cover": "jest --passWithNoTests --coverage --reporters jest-stare "
  },
  "dependencies": {
    "@vueuse/core": "^6.3.2",
    "axios": "^0.21.4",
    "bucket-storage": "^0.4.4",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.6",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.0-rc.8",
    "qs": "^6.10.1",
    "ramda": "^0.27.1",
    "tailwindcss": "^2.2.14",
    "typescript": "^4.4.2",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-dynamic-import-vars": "^1.4.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/crypto-js": "^4.0.2",
    "@types/jest": "^27.0.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.9.0",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.27.44",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-legacy": "^1.5.3",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vitejs/plugin-vue-jsx": "^1.1.8",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/test-utils": "^1.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.17.0",
    "jest": "^27.1.1",
    "jest-stare": "^2.2.2",
    "less": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.3.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "vite": "^2.5.10",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.0",
    "vue-tsc": "^0.3.0"
  },
  "jest-stare": {
    "merge": "true",
    "resultDir": "jest-stare",
    "log": "true",
    "resultHtml": "index.html",
    "resultJson": "data.json",
    "jestStareConfigJson": "jest-stare-config.json",
    "coverageLink": "../coverage/lcov-report/index.html"
  }
}
