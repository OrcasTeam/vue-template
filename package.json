{
  "name": "vite-template",
  "version": "1.0.0",
  "repository": "https://github.com/OrcasTeam/vue-template.git",
  "author": "yanzhangshuai <yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:skip-check": "vite build",
    "build": "vue-tsc --noEmit && vite build",
    "build:br": "vue-tsc --noEmit && cross-env VITE_BUILD_COMPRESS=brotli  vite build",
    "build:gzip": "vue-tsc --noEmit && cross-env VITE_BUILD_COMPRESS=gzip  vite build",
    "serve": "npm-run-all build && vite preview",
    "report": "cross-env VITE_REPORT=true  vite build",
    "lint:js": "eslint --config .eslintrc.js  ./{src,test,type,build}/**/*.{ts,tsx,vue} --fix",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{css,less,vue} --fix",
    "lint": "npm-run-all --parallel lint:*",
    "test": "jest --passWithNoTests",
    "test:cover": "jest --passWithNoTests --coverage --reporters jest-stare",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.31",
    "@vueuse/core": "^7.6.2",
    "axios": "^0.26.0",
    "bucket-storage": "^0.4.6",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.11",
    "qs": "^6.10.3",
    "ramda": "^0.27.2",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.5.5",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-dynamic-import-vars": "^1.4.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^27.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.17",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.27.64",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.5",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.18",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.17",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-stare": "^2.3.0",
    "lint-staged": "^12.3.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.0.2",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "rollup-plugin-visualizer": "^5.5.4",
    "stylelint": "^14.5.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.8.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.0.6",
    "vue-tsc": "^0.31.4"
  },
  "jest-stare": {
    "merge": "true",
    "resultDir": "jest-stare",
    "log": "true",
    "resultHtml": "index.html",
    "resultJson": "data.json",
    "jestStareConfigJson": "jest-stare-config.json",
    "coverageLink": "../coverage/lcov-report/index.html"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
