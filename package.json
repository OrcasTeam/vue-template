{
  "name": "vue-webpack",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/OrcasTeam/vue-template.git",
  "author": "yanzhangshuai <yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "ts-node": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\"",
    "dev": "yarn ts-node webpack serve",
    "build": "yarn lint-fix && yarn ts-node webpack",
    "build:br": "yarn lint-fix && yarn ts-node WEBPACK_BUILD_COMPRESS=brotli  webpack",
    "build:gzip": "yarn lint-fix && yarn ts-node WEBPACK_BUILD_COMPRESS=gzip  webpack",
    "report": "yarn ts-node WEBPACK_REPORT=true webpack",
    "lint:js": "eslint --config .eslintrc.js  ./{src,test}/**/*.{ts,tsx,vue}",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx}",
    "lint": "npm-run-all --parallel lint:*",
    "lint-fix:js": "eslint --config .eslintrc.js  ./src/**/*.{ts,tsx,vue} --fix",
    "lint-fix:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx} --fix",
    "lint-fix": "npm-run-all --parallel lint-fix:*"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.2",
    "axios": "^0.21.1",
    "core-js": "^3.15.2",
    "dayjs": "^1.10.6",
    "less": "^4.1.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.0-beta.5",
    "regenerator-runtime": "^0.13.7",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5",
    "vue": "^3.1.5",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-function-bind": "^7.14.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/axios": "^0.14.0",
    "@types/compression-webpack-plugin": "^6.0.6",
    "@types/css-minimizer-webpack-plugin": "^3.0.2",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/lodash-es": "^4.17.4",
